select * from rentals;
select * from books;
select * from users;
-- *Aliceê°€ ëŒ€ì—¬í•œ ì±…ì˜ ì œëª©ê³¼ ì €ìžë¥¼ ëª¨ë‘ ì¶œë ¥í•˜ì‹œì˜¤.*
select b.title,b.author from rentals r join users u 
on r.user_id = u.id join books b on r.book_id = b.id
where u.name='Alice';


-- âœ… ë¬¸ì œ 1: ì˜¨ë¼ì¸ ì˜í™” ìŠ¤íŠ¸ë¦¬ë° ì„œë¹„ìŠ¤

-- ðŸ“‹ ìš”êµ¬ì‚¬í•­
-- ì‚¬ìš©ìž(User)ëŠ” ì´ë¦„, ì´ë©”ì¼ì„ ê°€ì§„ë‹¤.
